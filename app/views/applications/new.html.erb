<h2>New Application</h2>
<%= form_with url: "/applications", method: :post, local: true do |f| %>
  <%= f.label :name %>&emsp;&nbsp
  <%= f.text_field :name %><br/><br/>

  <%= f.label :address %>
  <%= f.text_field :address %><br/><br/>

  <%= f.label :city %>&emsp;&nbsp&nbsp
  <%= f.text_field :city %><br/><br/>

  <%= f.label :state %>&emsp;&nbsp
  <%= f.text_field :state %><br/><br/>

  <%= f.label :zip_code %>
  <%= f.text_field :zip_code %><br/><br/>

  <%= f.submit "Create New" %>
<% end %>
